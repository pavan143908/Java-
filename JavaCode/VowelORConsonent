package triangle;

public class SwitchForVowel {

	public static void main(String[] args) {
		System.out.println(isVowel('U'));
	}

	public static boolean isVowel(char ch) {
		switch (ch) {
		case 'A':
		case 'E':
		case 'I':
		case 'O':
		case 'U':
		case 'a':
		case 'e':
		case 'i':
		case 'o':
		case 'u':
			return true;
		default:
			return false;
		}
	}
}
