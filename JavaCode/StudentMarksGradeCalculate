package student;

public class StudentMarks {

	public static void main(String[] args) {
		System.out.println(Student(99));
	}

	public static char Student(int marks) {
		if (marks > 100 || marks < 0) {
			return 'x';
		}
		if (marks >= 90) {
			return 'A';
		}
		if (marks >= 80) {
			return 'B';
		}
		if (marks >= 70) {
			return 'C';
		}
		if (marks >= 60) {
			return 'D';
		}
		if (marks >= 50) {
			return 'E';
		}
		if (marks < 50) {
			return 'F';
		}

		return 'y';
	}
}
